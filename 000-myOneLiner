#Subfinder (https://github.com/projectdiscovery/subfinder)
  ðŸ—²subfinder -d target.com
  ðŸ—²subfinder -d target.com -o subs.txt
  ðŸ—²subfinder -d target.com -silent -o subs.txt | httpx -title -content-length -status-code -silent

#Knockpy (https://github.com/guelfoweb/knock)
  ðŸ—²python3 knockpy.py target.com
  ðŸ—²python3 knockpy.py target.com | tee subs.txt

#Crtsh (https://github.com/YashGoti/crtsh)
  ðŸ—²python3 crtsh.py -d target.com

#Amass (https://github.com/owasp-amass/amass)
  ðŸ—²amass enum -passive -d target.com
  ðŸ—²amass enum -brute -d target.com
  ðŸ—²amass enum -o amass.txt -d target.com
  ðŸ—²amass enum -brute -active -d target.com -o amass.txt

ðŸ—²  ðŸ—²  ðŸ—²  ðŸ—²  ðŸ—²  ðŸ—²
#Httpx (https://github.com/projectdiscovery/httpx)
  ðŸ—²cat subdomains.txt | httpx -title -content-length -status-code -silent
  ðŸ—²httpx -l subdomains.txt -ports 80,8080,8000,8888 -threads 200 > subdomains_alive.txt

#Httprobe (https://github.com/tomnomnom/httprobe)
  ðŸ—²cat subdomains.txt | httprobe | tee -a subdomains_alive.txt
  ðŸ—²cat subdomains.txt | httprobe -p http:81 -p http:3000 -p https:3000 -p http:3001 -p https:3001 -p http:8000 -p http:8080 -p https:8443 -c 50 | tee subdomains_alive.txt

ðŸ—²  ðŸ—²  ðŸ—²  ðŸ—²  ðŸ—²  ðŸ—²
#Dirsearch (https://github.com/maurosoria/dirsearch)
  ðŸ—²dirsearch -u target.com
  ðŸ—²dirsearch -L allsubdomains.txt
  ðŸ—²dirsearch -L allsubdomains.txt -e json,html -r -x 500,400 -t 100
  ðŸ—²dirsearch -L allsubdomains.txt -e json,html -r -x 500,400 -t 100 -w /home/wordlists/common.txt

#Fuff (https://github.com/ffuf/ffuf)
  ðŸ—²ffuf -w /home/wordlists/common.txt -u https://target.com/FUZZ -e .php,.php.bak,.js,.json,.txt,.sql,.tar.gz,.bkp,.html,.htm,.zip -mc 200,301 -ac
  ðŸ—²ffuf -u https://targett.com/FUZZ -w /usr/share/wordlists/dirb/common.txt/
  ðŸ—²ffuf -u https://target.com/admin/FUZZ -w /home/wordlists/common.txt -mc 200
  ðŸ—²ffuf -w /home/wordlists/common.txt -u https://target.com/FUZZ -fs 1111 => does not show sizes 1111

#Paramspider (https://github.com/devanshbatham/ParamSpider)
  Discover directories for a single domain:
  ðŸ—²paramspider -d example.com
  Discover URLs for multiple domains from a file:
  ðŸ—²paramspider -l domains.txt

  ðŸ—²paramspider -d target.com --exclude png,jpg,gif,jpeg,swf,woff,gif,svg --level high --quiet -o result.txt

#Feroxbuster (https://github.com/epi052/feroxbuster)
  ðŸ—²./feroxbuster --url http://target.com --wordlist /home/wordlist/big.txt


  











